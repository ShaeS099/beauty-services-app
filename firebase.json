{
  "firestore": {
    "database": "(default)",
    "location": "eur3",
    "rules": "backend/firestore.rules",
    "indexes": "backend/firestore.indexes.json"
  },
  "functions": [
    {
      "source": "backend/functions",
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ],
      "codebase": "default"
    }
  ],
  "storage": {
    "rules": "backend/storage.rules"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  }
}


